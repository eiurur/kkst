{
  "name": "kkst",
  "version": "0.0.1",
  "description": "scribble code for node.js ",
  "main": "./build/index.js",
  "scripts": {
    "babel:build": "babel src --out-dir build",
    "babel:watch": "babel src --out-dir build --watch",
    "node:watch": "nodemon -w build/index.js",
    "dev": "concurrently --kill-others \"npm run babel:watch\" \"npm run node:watch\"",
    "start": "node build/index.js",
    "test": "ava -v -T=2m -C test/**",
    "lint": "eslint src/** --ext .js -c .eslintrc || true"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/eiurur/kkst.git"
  },
  "author": "eiurur",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.18.0",
    "concurrently": "^3.1.0",
    "eslint": "^3.8.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "nodemon": "^1.11.0"
  },
  "dependencies": {}
}
