{
  "name": "kkst",
  "version": "0.1.0",
  "description": "scribble code for node.js ",
  "main": "./build/index.js",
  "scripts": {
    "build": "babel src -d build",
    "babel:watch": "babel -w src -d build",
    "node:watch": "nodemon -w build/index.js",
    "dev": "concurrently --kill-others \"npm run babel:watch\" \"npm run node:watch\"",
    "start": "node build/index.js",
    "test": "ava -v -T=2m -C test/**",
    "lint": "eslint src/** --ext .js -c .eslintrc || true",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/eiurur/kkst.git"
  },
  "author": "eiurur",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.24.0",
    "concurrently": "^3.4.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.2.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {}
}
